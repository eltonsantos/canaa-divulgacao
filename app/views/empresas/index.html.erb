<p id="notice"><%= notice %></p>

<h1>Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nomeempresa</th>
      <th>Nomeresponsavel</th>
      <th>Categoria</th>
      <th>Cep</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Telefone</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nomeEmpresa %></td>
        <td><%= empresa.nomeResponsavel %></td>
        <td><%= empresa.categoria %></td>
        <td><%= empresa.cep %></td>
        <td><%= empresa.endereco %></td>
        <td><%= empresa.bairro %></td>
        <td><%= empresa.telefone %></td>
        <td><%= empresa.descricao %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
